# Database Configuration
DATABASE_URL="postgresql://gatekeeper_user:gatekeeper_password@localhost:5433/gatekeeper"

# Cloudflare Configuration
# Get your API token from: https://dash.cloudflare.com/profile/api-tokens
# Get your account ID from: https://dash.cloudflare.com/ (right sidebar under API)
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here

# Token Encryption Key (generate with: openssl rand -hex 32)
TOKEN_ENCRYPTION_KEY=your_32_byte_hex_key_here